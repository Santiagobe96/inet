%description:
Tests LinearClock:

%#--------------------------------------------------------------------------------------------------------------
%inifile: omnetpp.ini
[General]
ned-path = .;../../../../src;../../lib
network = inet.test.unittest.lib.ClockTestNetwork
cmdenv-express-mode = false
cmdenv-log-prefix = "C:%C: "

*.host.clock.typename = "LinearClock"
*.host.clock.timeShift = 0.03s
*.host.clock.driftRate = 0 ppm
%#--------------------------------------------------------------------------------------------------------------
%postrun-command: grep "^C:" test.out > clock.out || true
%#--------------------------------------------------------------------------------------------------------------
%contains: clock.out
C:(ClockTestApp)ClockTestNetwork.host.app: start
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 1.1, scheduled simtime: 1.07, scheduled clock: 1.1
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 2.11, scheduled simtime: 2.08, scheduled clock: 2.11
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 2.89, scheduled simtime: 2.86, scheduled clock: 2.89
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 2.9999999, scheduled simtime: 2.9699999, scheduled clock: 2.9999999
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 3.11, scheduled simtime: 3.08, scheduled clock: 3.11
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 1.07, clock: 1.1
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 2.08, clock: 2.11
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 2.86, clock: 2.89
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 2.9699999, clock: 2.9999999
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 3.08, clock: 3.11
C:(ClockTestApp)ClockTestNetwork.host.app: finished: simtime: 3.08, clock: 3.11
%#--------------------------------------------------------------------------------------------------------------
%not-contains: stdout
undisposed object:
%not-contains: stdout
-- check module destructor
%#--------------------------------------------------------------------------------------------------------------
