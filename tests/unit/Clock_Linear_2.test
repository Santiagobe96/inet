%description:
Tests LinearClock:

%#--------------------------------------------------------------------------------------------------------------
%inifile: omnetpp.ini
[General]
ned-path = .;../../../../src;../../lib
network = inet.test.unittest.lib.ClockTestNetwork
cmdenv-express-mode = false
cmdenv-log-prefix = "C:%C: "

*.host.clock.typename = "LinearClock"
*.host.clock.timeShift = 0s
*.host.clock.driftRate = 1e6 ppm
%#--------------------------------------------------------------------------------------------------------------
%postrun-command: grep "^C:" test.out > clock.out || true
%#--------------------------------------------------------------------------------------------------------------
%contains: clock.out
C:(ClockTestApp)ClockTestNetwork.host.app: start
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 1.1, scheduled simtime: 0.55, scheduled clock: 1.1
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 2.11, scheduled simtime: 1.055, scheduled clock: 2.11
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 2.89, scheduled simtime: 1.445, scheduled clock: 2.89
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 2.9999999, scheduled simtime: 1.49999995, scheduled clock: 2.9999999
C:(ClockTestApp)ClockTestNetwork.host.app: schedule to clock: 3.11, scheduled simtime: 1.555, scheduled clock: 3.11
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 0.55, clock: 1.1
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 1.055, clock: 2.11
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 1.445, clock: 2.89
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 1.49999995, clock: 2.9999999
C:(ClockTestApp)ClockTestNetwork.host.app: arrived: simtime: 1.555, clock: 3.11
C:(ClockTestApp)ClockTestNetwork.host.app: finished: simtime: 1.555, clock: 3.11
%#--------------------------------------------------------------------------------------------------------------
%not-contains: stdout
undisposed object:
%not-contains: stdout
-- check module destructor
%#--------------------------------------------------------------------------------------------------------------
